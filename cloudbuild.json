{
    "steps": [
    {
        "name": "gcr.io/cloud-builders/git",
        "args": [
            "clone",
            "https://source.cloud.google.com/vue-do-web/vue-do"
        ],
        "env": [
            "PROJECT_ROOT=vue-do"
        ]
    },
    {
        "name": "gcr.io/cloud-builders/yarn",
        "args": [
            "build",
            "-t",
            "gcr.io/vue-do-web/vue-do",
            "."
        ]
    }
    ]
}